<Application x:Class="MClock.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MClock">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="TimelineColourKey" Color="Red"></SolidColorBrush>
            <SolidColorBrush x:Key="BacklineColourKey" Color="Green"></SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>